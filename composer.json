{
    "name": "ashishpatel1992/sfs-shield",
    "description": "Blocks spam user registrations using the StopForumSpam API. Features real-time IP and email checks, manual scans, detailed logging and bulk user scanning.",
    "type": "wordpress-plugin",
    "version": "1.0.0",
    "require": {
        "php": ">=7.4",
        "composer/installers": "^2.0"
    },
    "keywords": [
        "wordpress",
        "plugin",
        "spam",
        "security"
    ],
    "homepage": "https://ashishpatel.dev/stop-forum-spam",
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "squizlabs/php_codesniffer": "^3.6",
        "phpcompatibility/php-compatibility": "^9.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "wp-coding-standards/wpcs": "^3.1"
    },
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "Ashishpatel1992\\SFSShield\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "vendor/bin/phpcs",
        "fix": "vendor/bin/phpcbf"
    },
    "authors": [
        {
            "name": "Ashish Patel"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
